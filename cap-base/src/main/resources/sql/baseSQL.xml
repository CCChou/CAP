<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<util:map id="baseSql" map-class="java.util.HashMap"
		key-type="java.lang.String">
		<entry key="Sequence.findByNodeName">
			<value><![CDATA[select * from Sequence where nodeName = :nodeName]]></value>
		</entry>
		<entry key="Sequence.updateByNodeNameAndNextSeq">
			<value><![CDATA[update Sequence seq set seq.nextSeq=:nextSeq,seq.rounds=:rounds,seq.lastModifyTime=:lastModifyTime where seq.nodeName=:nodeName and seq.nextSeq=:oldSeq]]></value>
		</entry>
		<entry key="Sequence.insert">
			<value><![CDATA[INSERT INTO SEQUENCE (NODENAME, NEXTSEQ, ROUNDS,LASTMODIFYTIME ) VALUES (:nodeName,:nextSeq,:rounds,:lastModifyTime)]]></value>
		</entry>
	</util:map>

</beans>
