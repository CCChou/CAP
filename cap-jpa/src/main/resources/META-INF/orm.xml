<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
 http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
	version="1.0">
	<!-- settings by entity -->
	<entity metadata-complete="false" class="com.iisigroup.cap.base.model.CodeType">
		<!-- HQL -->
		<named-query name="getCodeOnly">
			<query>
                from CodeType as code where code.cdeType = :cdeType
            </query>
		</named-query>
		<!-- native sql <named-native-query name="Code.selectAll.native" result-class="com.bqd.mci.model.CodeType"> 
			<query> select code.* from CODETYPE code </query> </named-native-query> -->
	</entity>
</entity-mappings>
