<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">
	
	<bean id="userSqlStatement" class="com.iisigroup.cap.context.CapParameter">
		<constructor-arg>
			<util:set value-type="java.util.HashMap">
				<ref bean="otherSql"/>
			</util:set>
		</constructor-arg>
	</bean>
	<util:map id="otherSql" map-class="java.util.HashMap" key-type="java.lang.String" >
		<entry key="Sequence.findByNodeName">
			<value><![CDATA[select * from Sequence where nodeName = ?]]></value>
		</entry>
		<entry key="Sequence.updateByNodeNameAndNextSeq">
			<value><![CDATA[update Sequence set nextSeq=?,rounds=?,lastModifyTime=? where nodeName=? and nextSeq=?]]></value>
		</entry>
		<entry key="Sequence.insert">
			<value><![CDATA[INSERT INTO SEQUENCE ( OID, NODENAME, NEXTSEQ, ROUNDS,LASTMODIFYTIME ) VALUES (?,?,?,?,?)]]></value>
		</entry>
		<entry key="LogRec.insert">
			<value><![CDATA[INSERT INTO LOGREC ( oid, uri , reqtData, status,userId,startTm) VALUES (?,?,?,?,?,?)]]></value>
		</entry>
		<entry key="LogRec.update">
			<value><![CDATA[UPDATE LOGREC SET respData=?, status=?, endTm=? where oid=?]]></value>
		</entry>
	</util:map>

</beans>
